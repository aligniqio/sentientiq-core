# Provider keys
OPENAI_API_KEY=sk-openai...
ANTHROPIC_API_KEY=sk-ant...
GROQ_API_KEY=gsk_...

# Model choices (override to taste)
OPENAI_MODEL=gpt-4.1
ANTHROPIC_MODEL=claude-3-5-sonnet-20240620
